{
  "name": "google-cloud-kvstore",
  "version": "3.0.0",
  "description": "Use @google-cloud/datastore as a key-value store.",
  "main": "src/index.js",
  "files": [
    "src/"
  ],
  "scripts": {
    "cover": "nyc --reporter=lcov mocha test/*.js && nyc report",
    "docs": "jsdoc -c .jsdoc.js",
    "lint": "eslint src/ test/ system-test/ samples/",
    "prettier": "prettier --write src/*.js test/*.js system-test/*.js samples/*.js",
    "test": "mocha test",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "system-test": "mocha system-test --timeout 600000"
  },
  "repository": "stephenplusplus/google-cloud-kvstore",
  "keywords": [
    "gcloud",
    "google",
    "cloud",
    "datastore",
    "@google-cloud/datastore",
    "keyvalue",
    "store"
  ],
  "author": "Stephen Sawchuk",
  "license": "MIT",
  "devDependencies": {
    "codecov": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "ink-docstrap": "^1.3.2",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.14.0"
  }
}
